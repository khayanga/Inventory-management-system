
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum role {
  Admin
  Client
}


model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String   @unique
  password  String
  role      String   @default("Client")
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt

}


